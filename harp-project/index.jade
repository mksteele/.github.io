//- Include mixins from global mixins file
include _includes/_mixins

block nav-content
    p.h1 mksteele

block content
    .main-content
        #top-content
            #nightsky
                +canvas('nightsky.pde')
            #photo1
                +image("exhibit1.jpg", "Exhibit 1", 615, 450)

        #bottom-content
            #photo2
                +image("exhibit2.jpg", "Exhibit 2", 535, 233)

            #tati-photo
                +image("tati.gif", "Tati gif")